{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ACTIONPACK_250",
  "type": "object",
  "required": ["version", "pack", "statement", "principios", "estrutura", "blocos"],
  "properties": {
    "version": { "type": "string" },
    "pack": { "type": "string" },
    "statement": { "type": "string" },
    "principios": { "type": "array", "items": { "type": "string" } },
    "estrutura": {
      "type": "object",
      "required": ["blocos", "acoes_por_bloco", "total", "formato_id"],
      "properties": {
        "blocos": { "type": "integer" },
        "acoes_por_bloco": { "type": "integer" },
        "total": { "type": "integer" },
        "formato_id": { "type": "string" }
      }
    },
    "blocos": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "nome", "range"],
        "properties": {
          "id": { "type": "string" },
          "nome": { "type": "string" },
          "range": { "type": "string" }
        }
      }
    }
  }
}
