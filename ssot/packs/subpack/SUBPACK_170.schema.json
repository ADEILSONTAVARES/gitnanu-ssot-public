{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "SUBPACK_170",
  "type": "object",
  "required": ["version", "pack", "statement", "estrutura", "categorias"],
  "properties": {
    "version": { "type": "string" },
    "pack": { "type": "string" },
    "statement": { "type": "string" },
    "estrutura": {
      "type": "object",
      "required": ["total", "formato_id"],
      "properties": {
        "total": { "type": "integer" },
        "formato_id": { "type": "string" }
      }
    },
    "categorias": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "range"],
        "properties": {
          "id": { "type": "string" },
          "range": { "type": "string" }
        }
      }
    }
  }
}
